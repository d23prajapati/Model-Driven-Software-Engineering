<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:properties="http://www.eclipse.org/sirius/properties/1.0.0" xmlns:properties-ext-widgets-reference="http://www.eclipse.org/sirius/properties/1.0.0/ext/widgets/reference" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:validation="http://www.eclipse.org/sirius/description/validation/1.1.0" name="project" version="12.0.0.2017041100">
  <ownedViewpoints name="EmbeddedSystemViewpoint">
    <validationSet name="ModelCheck">
      <ownedRules xsi:type="validation:SemanticValidationRule" name="CheckWire" level="ERROR" message="There is a short circuit" targetClass="embeddedSystem::Connection">
        <audits auditExpression="aql: if (self.src.type=embeddedSystem::PinType::GND) or (self.target.type=embeddedSystem::PinType::GND) then not((self.src.type =embeddedSystem::PinType::VCC) or (self.target.type =embeddedSystem::PinType::VCC)) else true endif"/>
      </ownedRules>
    </validationSet>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="SystemStructureDiagram" domainClass="embeddedSystem::SystemStructure" enablePopupBars="true">
      <metamodel href="http://www.example.org/embeddedSystem#/"/>
      <defaultLayer name="Default">
        <nodeMappings name="ComponentNode" semanticCandidatesExpression="aql:self.components" domainClass="embeddedSystem::Component" reusedBorderedNodeMappings="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ComponentNode']/@borderedNodeMappings[name='PinGroupBorderedNode']">
          <borderedNodeMappings name="PinGroupBorderedNode" semanticCandidatesExpression="feature:eAllContents" domainClass="embeddedSystem::PinGroup">
            <borderedNodeMappings name="PinBoardNode" semanticCandidatesExpression="feature:eAllContents" domainClass="embeddedSystem::Pin">
              <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <forbiddenSides>SOUTH</forbiddenSides>
                <forbiddenSides>NORTH</forbiddenSides>
                <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
              </style>
              <conditionnalStyles predicateExpression="aql:self.type=embeddedSystem::PinType::VCC">
                <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelPosition="node" resizeKind="NSEW">
                  <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
                </style>
              </conditionnalStyles>
            </borderedNodeMappings>
            <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" labelExpression="aql:''" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </borderedNodeMappings>
          <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="BoardNode" domainClass="embeddedSystem::Board" reusedBorderedNodeMappings="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ComponentNode']/@borderedNodeMappings[name='PinGroupBorderedNode']">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql:self.name" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/embeddedSystem.project.design/arduinoImages/images/arduino.png">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="WireEdge" semanticCandidatesExpression="aql:self.connections" sourceMapping="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ComponentNode']/@borderedNodeMappings[name='PinGroupBorderedNode']/@borderedNodeMappings[name='PinBoardNode']" targetMapping="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ComponentNode']/@borderedNodeMappings[name='PinGroupBorderedNode']/@borderedNodeMappings[name='PinBoardNode']" targetFinderExpression="aql:self.target" sourceFinderExpression="aql:self.src" domainClass="embeddedSystem::Connection" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="embeddedSystemTools">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Component" nodeMappings="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ComponentNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="embeddedSystem::Component" referenceName="components">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'New Component'"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="createPin" precondition="aql:container.oclIsKindOf(embeddedSystem::PinGroup) or container.oclIsKindOf(embeddedSystem::Component) or container.oclIsKindOf(embeddedSystem::Board)" nodeMappings="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ComponentNode']/@borderedNodeMappings[name='PinGroupBorderedNode']/@borderedNodeMappings[name='PinBoardNode']" extraMappings="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode'] //@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ComponentNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Switch">
                <cases conditionExpression="aql:container.oclIsKindOf(embeddedSystem::PinGroup)">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:container.oclAsType(embeddedSystem::PinGroup)">
                    <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                      <subModelOperations xsi:type="tool_1:CreateInstance" typeName="embeddedSystem::Pin" referenceName="pins">
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="10"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:container.oclIsKindOf(embeddedSystem::Board) or container.oclIsKindOf(embeddedSystem::Component)">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="embeddedSystem::PinGroup" referenceName="pinGroups" variableName="groupInstance">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:groupInstance">
                        <subModelOperations xsi:type="tool_1:CreateInstance" typeName="embeddedSystem::Pin" referenceName="pins" variableName="pinInstance">
                          <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:pinInstance">
                            <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="10"/>
                          </subModelOperations>
                        </subModelOperations>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </cases>
                <default>
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="embeddedSystem::Pin" referenceName="pins" variableName="pininstance">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr"/>
                    </subModelOperations>
                  </subModelOperations>
                </default>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="createWire" edgeMappings="//@ownedViewpoints[name='EmbeddedSystemViewpoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@edgeMappings[name='WireEdge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.ancestors()->filter(embeddedSystem::SystemStructure)->first()">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="embeddedSystem::Connection" referenceName="connections">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:instance">
                    <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:instance">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="src" valueExpression="var:source"/>
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="target" valueExpression="var:target"/>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:ToolDescription" name="do">
            <element name="element"/>
            <elementView name="elementView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.oclAsType(embeddedSystem::Component).hello()"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="embeddedSystem.project.design.Services"/>
  </ownedViewpoints>
  <extensions xsi:type="properties:ViewExtensionDescription" name="Properties">
    <categories name="Default Category">
      <pages name="ComponentPage" labelExpression="Component Setup" domainClass="embeddedSystem::Component" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0 //@extensions.0/@categories.0/@groups.2">
        <validationSet>
          <semanticValidationRules name="NameCheck" level="ERROR" message="Empty Name" targetClass="embeddedSystem::Component">
            <audits auditExpression="aql:self.name.size()>0"/>
          </semanticValidationRules>
        </validationSet>
      </pages>
      <pages name="BoardPage" labelExpression="Board Settings" domainClass="embeddedSystem::Board" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.1 //@extensions.0/@categories.0/@groups.0"/>
      <groups name="GeneralGroup" labelExpression="General Setup" domainClass="EObject" semanticCandidateExpression="var:self" preconditionExpression="aql:self.oclIsKindOf(embeddedSystem::Component) or self.oclIsKindOf(embeddedSystem::Board)">
        <controls xsi:type="properties:LabelDescription" name="nameLabel" labelExpression="Name"/>
        <controls xsi:type="properties:TextDescription" name="nameText" labelExpression="Name" valueExpression="aql: if self.oclIsKindOf(embeddedSystem::Component) then self.oclAsType(embeddedSystem::Component).name else if self.oclIsKindOf(embeddedSystem::Board) then self.oclAsType(embeddedSystem::Board).name else 'unknown' endif endif">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
      </groups>
      <groups name="BoardGroup" labelExpression="Board Settings" domainClass="embeddedSystem::Board" semanticCandidateExpression="var:self">
        <controls xsi:type="properties:TextDescription" name="NameText" labelExpression="Board Name" valueExpression="aql:self.name">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
      </groups>
      <groups name="ComponentSettingGroup" labelExpression="Group" domainClass="embeddedSystem::Component" semanticCandidateExpression="var:self">
        <controls xsi:type="properties:RadioDescription" name="ComponentTypeRadio" labelExpression="Type" valueExpression="aql:self.type.toString()" candidatesExpression="aql:embeddedSystem::ComponentType.eContents().toString()" candidateDisplayExpression="aql:candidate.toString()">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool_1:SetValue" featureName="type" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
        <controls xsi:type="properties-ext-widgets-reference:ExtReferenceDescription" name="PinsReference" labelExpression="Pins" referenceNameExpression="pins" referenceOwnerExpression="aql:self"/>
      </groups>
    </categories>
  </extensions>
</description:Group>
